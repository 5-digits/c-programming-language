setup:
	mkdir out

ex-1-4:
	gcc ex-1-4.c -o out/ex-1-4

fahrenheit-to-celsius:
	gcc fahrenheit-to-celsius.c -o out/fahrenheit-to-celsius

fahrenheit-to-celsius-floats:
	gcc fahrenheit-to-celsius-floats.c -o out/fahrenheit-to-celsius-floats

hello-world:
	gcc hello-world.c -o out/hello-world

clean:
	rm out/*